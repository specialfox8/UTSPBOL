<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kacamata.FXMLDocumentController">
   <children>
      <MenuBar layoutY="2.0">
        <menus>
          <Menu mnemonicParsing="false" text="Data">
            <items>
              <MenuItem fx:id="data" mnemonicParsing="false" onAction="#dataklik" text="DataKaca" />
                  <MenuItem fx:id="pelanggan" mnemonicParsing="false" onAction="#pelangganklik" text="DataPelanggan" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Order">
               <items>
                  <MenuItem fx:id="beli" mnemonicParsing="false" onAction="#beliklik" text="Beli" />
               </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Keluar">
            <items>
              <MenuItem fx:id="keluar" mnemonicParsing="false" onAction="#keluarklik" text="Keluar" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
